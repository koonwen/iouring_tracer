(executable
 (public_name obpf)
 (name obpf)
 (libraries cmdliner obpftrace logs logs.fmt logs.cli fmt.tty dune-site)
 (modules obpf bt mysites))

(rule
 (target bt.ml)
 (deps
  (source_tree bt))
 (action
  (with-stdout-to
   %{target}
   (run ocaml-crunch -m plain bt))))

(generate_sites_module
 (module mysites)
 (sites obpftrace))
